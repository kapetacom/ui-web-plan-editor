{
    "name": "@kapeta/ui-web-plan-editor",
    "version": "2.3.1",
    "main": "src/index.ts",
    "files": [
        "package.json",
        "README.md",
        "src"
    ],
    "description": "Provides ReactJS components for viewing and editing Kapeta plans",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "kapeta"
    ],
    "license": "BUSL-1.1",
    "author": {
        "email": "engineering@kapeta.com",
        "name": "Kapeta",
        "url": "https://kapeta.com"
    },
    "scripts": {
        "prebuild": "rm -rf dist",
        "storybook": "storybook dev --port=54321",
        "build-storybook": "storybook build",
        "prepublishOnly": "npm run build",
        "build": "tsc",
        "format": "prettier --write .",
        "lint": "npm run lint:tsc && npm run lint:eslint",
        "lint:tsc": "tsc --noEmit",
        "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx ./",
        "test": "jest"
    },
    "prettier": "@kapeta/prettier-config",
    "peerDependencies": {
        "@kapeta/kaplang-core": "^1.9",
        "@kapeta/nodejs-utils": "<2",
        "@kapeta/schemas": "^3",
        "@kapeta/ui-web-components": "^3",
        "@kapeta/ui-web-context": "^1",
        "@kapeta/ui-web-types": "^1",
        "@kapeta/ui-web-utils": ">=0.0.29 <2",
        "@mui/icons-material": "^5",
        "@mui/material": "^5",
        "guid-typescript": "*",
        "less": ">=4",
        "lodash": ">=4",
        "react": ">=18",
        "react-dom": ">=18",
        "react-tabs": ">=6",
        "react-use": ">=17"
    },
    "dependencies": {
        "@types/d3-selection": "^3.0.10",
        "@types/d3-transition": "^3.0.8",
        "@types/d3-zoom": "^3.0.8",
        "@types/lodash": "^4.14.185",
        "@types/node": "^20.8.9",
        "@types/pathfinding": "^0.0.8",
        "@types/react": ">=18",
        "@types/react-dom": ">=18",
        "d3-selection": "^3.0.0",
        "d3-transition": "^3.0.1",
        "d3-zoom": "^3.0.0",
        "pathfinding": "^0.4.18"
    },
    "devDependencies": {
        "@kapeta/kaplang-core": "^1.11.2",
        "@kapeta/nodejs-utils": "<2",
        "@kapeta/prettier-config": "^0.6.2",
        "@kapeta/style": "^0.94.0",
        "@mui/icons-material": "^5.14.3",
        "@mui/material": "^5.14.3",
        "@sambego/storybook-state": "^2.0.1",
        "@spotify/eslint-config": "^14.1.6",
        "@storybook/addon-controls": "^6.5.16",
        "@storybook/addon-essentials": "^7.5.0",
        "@storybook/addon-styling": "^1.3.7",
        "@storybook/addon-styling-webpack": "^0.0.5",
        "@storybook/builder-webpack5": "^6.5.10",
        "@storybook/manager-webpack5": "^6.5.0-rc.1",
        "@storybook/react": "^6.5.16",
        "@storybook/react-webpack5": "^7.5.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.35.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.7.0",
        "json-loader": "^0.5.7",
        "less-loader": "^11.0.0",
        "prettier": "^2.8.8",
        "react-error-boundary": "^3.1.4",
        "storybook": "^7.5.0",
        "style-loader": "^3.3.1",
        "ts-jest": "^29.1.0",
        "typescript": "^4.8.3",
        "yaml": "^2.1.1",
        "yaml-loader": "^0.8.0"
    },
    "eslintConfig": {
        "extends": [
            "@spotify"
        ],
        "plugins": [
            "unused-imports"
        ],
        "rules": {
            "react/no-unused-prop-types": "error",
            "unused-imports/no-unused-imports": "error"
        },
        "ignorePatterns": [
            "dist/**"
        ]
    },
    "release": {
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            [
                "@semantic-release/changelog",
                {
                    "changelogFile": "CHANGELOG.md"
                }
            ],
            "@semantic-release/npm",
            "@semantic-release/github",
            [
                "@semantic-release/git",
                {
                    "assets": [
                        "CHANGELOG.md",
                        "package.json",
                        "package-lock.json"
                    ],
                    "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
                }
            ]
        ]
    }
}
